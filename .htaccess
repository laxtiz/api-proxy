# Apache .htaccess configuration for PHP Proxy
# This file enables URL rewriting

# Enable mod_rewrite if not already enabled
RewriteEngine On

# Skip existing files and directories to serve them directly
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d

# Rewrite all other requests to proxy.php with QSA (Query String Append)
RewriteRule .* proxy.php [L,QSA]

